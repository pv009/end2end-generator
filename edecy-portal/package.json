{
  "name": "edecy-portal",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:prod": "ng serve --prod",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "cypress": "start-server-and-test start http-get://localhost:4200 cy:run",
    "cy:open": "cypress open",
    "cy:run": "cypress run --spec='cypress/integration/auth-tests/auth-login-test.spec.js' -b chrome",
    "build:prerender": "npm run build:client-and-server-bundles && npm run compile:server && npm run generate:prerender",
    "generate:prerender": "cd dist && node prerender",
    "serve:prerender": "cd dist/browser && http-server",
    "compile:server_bak": "webpack --config webpack.server.config.js --progress --colors",
    "build:ssr_bak": "npm run build:client-and-server-bundles && npm run compile:server",
    "serve:ssr_bak": "node dist/server",
    "build:client-and-server-bundles_bak": "ng build --prod && ng run edecy-portal:server:production --bundleDependencies all",
    "dev:ssr": "ng run edecy-portal:serve-ssr",
    "serve:ssr": "node dist/server/main.js",
    "build:ssr": "ng build --prod && ng run edecy-portal:server:production",
    "build:ssr:staging": "ng build --configuration=staging && ng run edecy-portal:server:production",
    "prerender": "ng run edecy-portal:prerender",
    "test:ci": "ng test --no-watch --no-progress --browsers=ChromeHeadlessNoSandbox",
    "jest": "jest",
    "jest:watch": "jest --watch",
    "jest:ci": "jest --runInBand"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.9",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "^9.1.9",
    "@angular/compiler": "^9.1.9",
    "@angular/core": "^9.1.9",
    "@angular/flex-layout": "^9.0.0-beta.31",
    "@angular/forms": "^9.1.9",
    "@angular/localize": "^9.1.9",
    "@angular/material": "^9.2.4",
    "@angular/platform-browser": "^9.1.9",
    "@angular/platform-browser-dynamic": "^9.1.9",
    "@angular/platform-server": "^9.1.9",
    "@angular/router": "^9.1.9",
    "@angular/service-worker": "^9.1.9",
    "@nguniversal/express-engine": "^9.1.1",
    "@nguniversal/module-map-ngfactory-loader": "^8.2.6",
    "@ngxs/logger-plugin": "^3.6.2",
    "@ngxs/storage-plugin": "^3.6.2",
    "@ngxs/store": "^3.6.2",
    "@schematics/angular": "^8.3.26",
    "@sentry/browser": "^5.15.5",
    "@sentry/integrations": "^5.15.5",
    "@types/jest": "^26.0.15",
    "@types/parse": "^2.12.1",
    "angular-froala-wysiwyg": "^3.1.1-1",
    "bootstrap": "^4.5.0",
    "domino": "^2.1.5",
    "express": "^4.15.2",
    "express-list-endpoints": "^4.0.1",
    "express-prom-bundle": "^5.1.5",
    "file-class": "^0.1.3",
    "fs": "0.0.1-security",
    "jest": "^26.6.3",
    "jest-environment-node": "^26.6.2",
    "jest-preset-angular": "^8.3.2",
    "mkdirp": "^1.0.4",
    "moment": "^2.26.0",
    "ngx-cookie-service": "^2.4.0",
    "ngx-image-cropper": "3.2.1",
    "ngx-image-gallery": "^2.0.5",
    "ngx-image-viewer": "^1.0.13",
    "ngx-image-zoom": "^0.6.0",
    "ngx-lightbox": "^2.1.2",
    "ngx-material-file-input": "^2.1.1",
    "ngx-matomo": "^0.1.4",
    "npm-check-updates": "^6.0.1",
    "os": "^0.1.1",
    "parse": "^2.13.0",
    "path": "^0.12.7",
    "prom-client": "^11.5.3",
    "rxjs": "^6.5.5",
    "screenfull": "^3.3.0",
    "tslib": "^1.13.0",
    "url-pattern": "^1.0.3",
    "xml-writer": "^1.7.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.8",
    "@angular-devkit/core": "^9.1.7",
    "@angular-devkit/schematics": "^9.1.7",
    "@angular/cli": "^9.1.7",
    "@angular/compiler-cli": "^9.1.9",
    "@angular/language-service": "^9.1.9",
    "@nguniversal/builders": "^9.1.1",
    "@ngxs/devtools-plugin": "^3.6.2",
    "@types/express": "^4.17.0",
    "@types/hammerjs": "^2.0.36",
    "@types/jasmine": "^3.5.10",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.2.2",
    "cypress": "^5.3.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^5.0.9",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "localstorage-polyfill": "^1.0.1",
    "protractor": "^7.0.0",
    "puppeteer": "^5.5.0",
    "start-server-and-test": "^1.11.0",
    "ts-loader": "^5.2.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.8.3",
    "webpack-cli": "^3.3.11"
  },
  "browser": {
    "fs": false,
    "crypto": false
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/src/e2e/setup/setupJest.js"
    ],
    "globalSetup": "<rootDir>/src/e2e/setup/setupJest.js",
    "globalTeardown": "<rootDir>/src/e2e/setup/teardown.js",
    "testEnvironment": "<rootDir>/src/e2e/setup/puppeteer_environment.js",
    "roots": ["<rootDir>/src/e2e/tests/"],
    "testMatch": ["**/+(*.)+(spec).+(ts)"]
  }
}
